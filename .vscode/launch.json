  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch dev-local",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/index.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "env": {
        "NODE_ENV": "development"
      },
      "protocol": "inspector",
      "smartStep": true,
      "console": "integratedTerminal"
    }
  ]
}
